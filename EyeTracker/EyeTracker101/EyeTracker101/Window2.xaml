        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EyeTracker101"
        mc:Ignorable="d"
        Title="Window2" Height="300" Width="300" WindowState="Maximized">
    <Grid>
        <Canvas Name="DisplayCanvas">
            <Button Width="300" Height="300" Background="Aqua" Canvas.Left="100" Canvas.Top="300" Name="rectSample"></Button>
            <TextBlock Width="100" Height="20" Name="txtPosition" Panel.ZIndex="100">Position</TextBlock>
        </Canvas>
    </Grid>
</Window>
