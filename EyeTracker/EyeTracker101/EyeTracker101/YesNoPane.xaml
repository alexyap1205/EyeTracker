<UserControl x:Class="EyeTracker101.YesNoPane"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EyeTracker101"
             xmlns:controls="clr-namespace:Presentation.Controls;assembly=Presentation"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <controls:OverlayedButton Name="btnYes" Margin="50" Width="500" Grid.Column="0"/>
        <controls:OverlayedButton Name="btnNo" Margin="50" Width="500" Grid.Column="1"/>
    </Grid>
</UserControl>
